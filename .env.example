# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Sheets API Configuration (for private spreadsheet access)
GOOGLE_PROJECT_ID=your_google_project_id
GOOGLE_PRIVATE_KEY_ID=your_google_private_key_id
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_private_key_here\n-----END PRIVATE KEY-----"
GOOGLE_CLIENT_EMAIL=your_service_account_email@your_project.iam.gserviceaccount.com
GOOGLE_CLIENT_ID=your_google_client_id

# Google Sheets Leaderboard Configuration
GOOGLE_SHEETS_LEADERBOARD_ID=your_spreadsheet_id_here
GOOGLE_SHEETS_GID=your_sheet_gid_here
GOOGLE_SHEETS_RANGE='SL'!A:AH

# Bunny.net Stream - Video Hosting
# HLS format (works on all browsers with HLS.js)
NEXT_PUBLIC_VIDEO_HERO_URL=https://vz-[library-id].b-cdn.net/[video-id]/playlist.m3u8
NEXT_PUBLIC_VIDEO_HYPE_URL=https://vz-[library-id].b-cdn.net/[video-id]/playlist.m3u8
